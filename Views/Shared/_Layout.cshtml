<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HallBookingBhatPara</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Custom.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Developer.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HallBookingBhatPara.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        @await Html.PartialAsync("_NavBar")        
    </header>
    @RenderBody()
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - HallBookingBhatPara - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <!-- scrips -->
    @await Html.PartialAsync("_Script") 
     
    
    @* @await RenderSectionAsync("Scripts", required: false) *@
</body>
</html>
